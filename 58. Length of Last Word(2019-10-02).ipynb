   "source": [
    "class Solution:\n",
    "    def lengthOfLastWord(self, s: str) -> int:\n",
    "        self.s = s.split()\n",
    "        if self.s == []:\n",
    "            return 0\n",
    "        else:\n",
    "            return len(self.s[-1])"
   ]
  },
